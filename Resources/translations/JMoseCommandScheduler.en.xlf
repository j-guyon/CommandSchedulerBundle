<?xml version="1.0"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="en" datatype="plaintext" original="file.ext">
        <body>

            <!-- Bundle -->
            <trans-unit id="1">
                <source>name</source>
                <target>Symfony's scheduler</target>
            </trans-unit>

            <trans-unit id="2">
                <source>dateFormat</source>
                <target>Y-m-d H:i</target>
            </trans-unit>

            <trans-unit id="3">
                <source>list.title</source>
                <target>Scheduled commands</target>
            </trans-unit>

            <!-- Actions -->
            <trans-unit id="100">
                <source>action.delete</source>
                <target>Remove</target>
            </trans-unit>
            <trans-unit id="101">
                <source>action.execute</source>
                <target>Execute</target>
            </trans-unit>
            <trans-unit id="102">
                <source>action.edit</source>
                <target>Edit</target>
            </trans-unit>
            <trans-unit id="103">
                <source>action.new</source>
                <target>New scheduling</target>
            </trans-unit>
            <trans-unit id="104">
                <source>confirm.delete</source>
                <target>Permanently delete this task ?</target>
            </trans-unit>
            <trans-unit id="105">
                <source>confirm.execute</source>
                <target>Force an execution for this task ?</target>
            </trans-unit>
            <trans-unit id="106">
                <source>action.unlock</source>
                <target>Unlock task</target>
            </trans-unit>
            <trans-unit id="107">
                <source>confirm.unlock</source>
                <target>The task may be running. Are you sure you want to unlock it ?</target>
            </trans-unit>

            <!-- Detail  -->
            <trans-unit id="201">
                <source>detail.title</source>
                <target>Scheduled command</target>
            </trans-unit>

            <trans-unit id="202">
                <source>detail.name</source>
                <target>Name</target>
            </trans-unit>
            <trans-unit id="203">
                <source>detail.command</source>
                <target>Command</target>
            </trans-unit>
            <trans-unit id="204">
                <source>detail.arguments</source>
                <target>Arguments</target>
            </trans-unit>
            <trans-unit id="205">
                <source>detail.cronExpression</source>
                <target>Cron expression</target>
            </trans-unit>
            <trans-unit id="206">
                <source>detail.logFile</source>
                <target>Output file</target>
            </trans-unit>
            <trans-unit id="207">
                <source>detail.priority</source>
                <target>Priority</target>
            </trans-unit>
            <trans-unit id="208">
                <source>detail.executeImmediately</source>
                <target>Execute now</target>
            </trans-unit>
            <trans-unit id="209">
                <source>detail.disabled</source>
                <target>Disable</target>
            </trans-unit>
            <trans-unit id="210">
                <source>detail.save</source>
                <target>Save</target>
            </trans-unit>
            <trans-unit id="211">
                <source>detail.lastExecution</source>
                <target>Last run</target>
            </trans-unit>
            <trans-unit id="212">
                <source>detail.lastReturnCode</source>
                <target>Last return code</target>
            </trans-unit>
            <trans-unit id="213">
                <source>detail.actions</source>
                <target>Actions</target>
            </trans-unit>

            <!-- Flashs messages -->
            <trans-unit id="300">
                <source>flash.success</source>
                <target>Scheduling successfully saved.</target>
            </trans-unit>
            <trans-unit id="301">
                <source>flash.deleted</source>
                <target>Scheduling successfully deleted.</target>
            </trans-unit>
            <trans-unit id="302">
                <source>flash.execute</source>
                <target>Command will be execute during the next "scheduler:execute".</target>
            </trans-unit>
            <trans-unit id="303">
                <source>flash.unlocked</source>
                <target>Scheduling successfully unlocked.</target>
            </trans-unit>

        </body>
    </file>
</xliff>